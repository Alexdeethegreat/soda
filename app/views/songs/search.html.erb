<%= render  'shared/header' %>
  <h1>Search</h1>
  <div class="info search-info">
    <%= form_for(:query, url: search_path) do |f|%>
	  <%= f.label :Artist %>
	  <%= f.text_field :artist %>
	  <%= f.label :Song %>
	  <%= f.text_field :song %>
	  <%= f.submit("Search") %>
	<% end %>
  </div>
</div>
